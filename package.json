{
  "name": "alphastudio",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "web:tv": "EXPO_TV=1 expo start --web",
    "proxy": "node proxy-server.js",
    "dev:web": "concurrently \"npm run proxy\" \"npm run web\"",
    "build:android": "eas build --platform android",
    "build:android-mobile": "eas build --platform android --profile android-mobile",
    "build:android-tv": "eas build --platform android --profile android-tv",
    "build:ios": "eas build --platform ios",
    "build:all": "eas build --platform all",
    "build:web": "expo export --platform web",
    "build:web-tv": "EXPO_TV=1 expo export --platform web"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/native": "^7.1.20",
    "axios": "^1.13.2",
    "expo": "~54.0.25",
    "expo-av": "~16.0.7",
    "expo-keep-awake": "~15.0.7",
    "expo-linear-gradient": "~15.0.7",
    "expo-router": "~6.0.15",
    "expo-splash-screen": "~31.0.11",
    "expo-status-bar": "~3.0.8",
    "hls.js": "^1.6.15",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.0"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "concurrently": "^9.2.1",
    "typescript": "~5.9.2"
  },
  "private": true
}
